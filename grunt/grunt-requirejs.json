{
    "dev": {
        "options": {
            "baseUrl": "src/javascripts",
            "mainConfigFile": "src/javascripts/ng-admin.js",
            "include": ["ng-admin"],
            "name": "bower_components/almond/almond",
            "out": "./build/ng-admin.min.js",
            "optimize": "none",
            "preserveLicenseComments": false,
            "findNestedDependencies": true,
            "generateSourceMaps": false,
            "skipDirOptimize": true,
            "removeCombined": true,
            "wrap": {
                "startFile": "src/build/start-ngadmin.frag",
                "endFile": "src/build/end-ngadmin.frag"
            }
        }
    },
    "ngconfig": {
        "options": {
            "baseUrl": "build/es6/",
            "include": ["Factory.js"],
            "name": "../../src/javascripts/bower_components/almond/almond",
            "out": "build/ng-admin-configuration.js",
            "optimize": "none",
            "preserveLicenseComments": false,
            "findNestedDependencies": true,
            "generateSourceMaps": false,
            "skipDirOptimize": true,
            "removeCombined": true,
            "wrap": {
                "startFile": "src/build/start-configuration.frag",
                "endFile": "src/build/end-configuration.frag"
            }
        }
    },
    "prod": {
        "options": {
            "baseUrl": "src/javascripts",
            "mainConfigFile": "src/javascripts/ng-admin.js",
            "include": ["ng-admin"],
            "name": "bower_components/almond/almond",
            "out": "./build/ng-admin.min.js",
            "optimize": "none",
            "preserveLicenseComments": false,
            "findNestedDependencies": true,
            "generateSourceMaps": false,
            "skipDirOptimize": true,
            "removeCombined": true,
            "wrap": {
                "startFile": "src/build/start-ngadmin.frag",
                "endFile": "src/build/end-ngadmin.frag"
            }
        }
    },
    "prod-only": {
        "options": {
            "baseUrl": "src/javascripts",
            "mainConfigFile": "src/javascripts/ng-admin-only.js",
            "include": ["ng-admin-only"],
            "exclude": [
                "bower_components/angular-resource/angular-resource",
                "bower_components/angular-sanitize/angular-sanitize",
                "bower_components/angular-ui-router/release/angular-ui-router",
                "bower_components/ng-file-upload/angular-file-upload",
                "bower_components/lodash/dist/lodash.min",
                "bower_components/requirejs-text/text",
                "bower_components/requirejs-text/text",
                "bower_components/angular-bootstrap/ui-bootstrap-tpls.min",
                "bower_components/angular-bootstrap/ui-bootstrap.min",
                "bower_components/restangular/dist/restangular",
                "bower_components/ngInflection/ngInflection",
                "bower_components/inflection/inflection.min",
                "bower_components/humane/humane",
                "bower_components/nprogress/nprogress",
                "bower_components/textAngular/dist/textAngular.min",
                "bower_components/angular-ui-codemirror/ui-codemirror.min"
            ],
            "name": "bower_components/almond/almond",
            "out": "./build/ng-admin-only.min.js",
            "optimize": "none",
            "preserveLicenseComments": false,
            "findNestedDependencies": true,
            "generateSourceMaps": false,
            "skipDirOptimize": true,
            "removeCombined": true,
            "wrap": {
                "startFile": "src/build/start.frag",
                "endFile": "src/build/end.frag"
            }
        }
    }
}
